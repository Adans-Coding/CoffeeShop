{
  "expo": {
    "name": "coffeShop",
    "slug": "coffeShop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/CoffeeShotlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "icon": "./assets/Frame.png",
      "iconResizeMode": "contain",
      "iconSize": 200
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/CoffeeShotlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adanadil.coffeShop"
    },
    "web": {
      "favicon": "./assets/CoffeeShotlogo.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c6ede22e-1655-4580-b69c-6bf27cbe7330"
      }
    }
  }
}
